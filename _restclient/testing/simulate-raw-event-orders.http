@simulateRawEventBaseUrl = {{$dotenv SIMULATE_RAW_EVENT_BASE_URL}}

@orderId = LLL100

### POST ###
POST {{simulateRawEventBaseUrl}}/api/v1/testing/simulateRawEvent
Origin: https://localhost
Content-Type: application/json

{
 "pk": "ORDER_ID#{{orderId}}",
 "sk": "EVENT#ORDER_PLACED_EVENT",
 "eventName": "ORDER_PLACED_EVENT",
 "eventData": {
  "orderId": "{{orderId}}",
  "sku": "ORANGES",
  "units": 2,
  "price": 22.99,
  "userId": "123456"
 }
}

