{
  "name": "dynamodb-eventbridge-driven-ecomm-nodejs-result-infra",
  "version": "0.1.0",
  "config": {
    "deployment_prefix": "ddbEbEcommNjsRes"
  },
  "bin": {
    "main": "bin/main.ts"
  },
  "scripts": {
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json}'",
    "format:check": "prettier --check '**/*.{js,jsx,ts,tsx,json}'",
    "lint": "eslint . --ext '**/*.js,.jsx,.ts,.tsx' --ignore-pattern node_modules/",
    "test": "jest",
    "test:coverage": "jest --silent --coverage --verbose",
    "test:build": "run-s format:check lint test:coverage build",
    "watch": "tsc -w",
    "build": "tsc --build -v -f",
    "build:lint": "run-s format:check lint build",
    "bootstrap": "cross-env ts-node bin/cdk-runner.ts bootstrap",
    "synth": "cross-env ts-node bin/cdk-runner.ts synth",
    "deploy": "cross-env WRITE_ENV_FILES=true ts-node bin/cdk-runner.ts deploy",
    "destroy": "cross-env ts-node bin/cdk-runner.ts destroy"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.147",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.24",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "aws-cdk": "^2.1005.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.185.0",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.4.2",
    "source-map-support": "^0.5.21"
  }
}
