{
  "name": "edaof-infra",
  "version": "0.1.0",
  "config": {
    "prefix": "edaof"
  },
  "bin": {
    "main": "bin/main.ts"
  },
  "scripts": {
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json}'",
    "lint": "eslint . --ext '**/*.js,.jsx,.ts,.tsx' --ignore-pattern node_modules/",
    "build": "tsc --build -v -f",
    "watch": "tsc -w",
    "test": "jest",
    "dist": "esbuild",
    "bootstrap": "cross-env ts-node bin/cdk-runner.ts bootstrap",
    "synth": "cross-env ts-node bin/cdk-runner.ts synth",
    "deploy": "cross-env ts-node bin/cdk-runner.ts deploy",
    "destroy": "cross-env ts-node bin/cdk-runner.ts destroy"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.146",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.10",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "aws-cdk": "^2.173.4",
    "esbuild": "^0.24.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.114.1-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.114.1-alpha.0",
    "aws-cdk-lib": "^2.173.4",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.4.2",
    "cross-env": "^7.0.3",
    "source-map-support": "^0.5.21"
  }
}
